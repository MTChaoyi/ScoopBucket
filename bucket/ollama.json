{
    "version": "0.2.8",
    "description": "在本地启动和运行大型语言模型（带模型安装位置环境变量，需自行调整）",
    "homepage": "https://ollama.com/",
    "license": "MIT",
    "url": "https://github.com/ollama/ollama/releases/download/v0.2.8/OllamaSetup.exe",
    "hash": "c18cac79c0cb4cd3845ec538dc5e79215e296ce7cbffa48916b0b44b16790c67",
    "innosetup": true,
    "bin": "ollama.exe",
    "shortcuts": [
        [
            "ollama app.exe",
            "Ollama",
            "",
            "app.ico"
        ]
    ],
    "env_set": {
        "OLLAMA_MODELS": "D:\\$env:UserName\\Play\\AI\\ollama\\models"
    },
    "checkver": {
        "github": "https://github.com/ollama/ollama"
    },
    "autoupdate": {
        "url": "https://github.com/ollama/ollama/releases/download/v$version/OllamaSetup.exe"
    },
    "notes": "您应该至少有 8 GB 可用 RAM 来运行 7B 型号，16 GB 来运行 13B 型号，32 GB 来运行 33B 型号"
}
